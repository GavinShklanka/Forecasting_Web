{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Flask App",
      "type": "debugpy",
      "request": "launch",
      "program": "app.py",
      "console": "integratedTerminal",
      "env": {
        "FLASK_APP": "app.py",
        "FLASK_ENV": "development"
      },
      "cwd": "${workspaceFolder}",
      "justMyCode": true
    }
  ]
}
